<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation" xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">

    <!--  Background Colors  -->
    <Color x:Key="primaryBackColor1">#1B3A4B</Color>
    <Color x:Key="primaryBackColor2">#6D6D6D</Color>
    <Color x:Key="secondaryBackColor1">#6D6D6D</Color>
    <Color x:Key="secondaryBackColor2">#1B3A4B</Color>
    <!--  Window Border Colors  -->
    <Color x:Key="winBorderColor1">#00FFFF</Color>
    <Color x:Key="winBorderColor2">#008CFF</Color>
    <Color x:Key="winBorderColor3">#1338BE</Color>
    <!--  Color Palette  -->
    <SolidColorBrush x:Key="color1" Color="#E285DE" />
    <SolidColorBrush x:Key="color2" Color="#784DFD" />
    <SolidColorBrush x:Key="color3" Color="#4ADAEC" />
    <SolidColorBrush x:Key="color4" Color="#FB539B" />
    <SolidColorBrush x:Key="color5" Color="#7E82FC" />
    <SolidColorBrush x:Key="color6" Color="#FFC047" />
    <SolidColorBrush x:Key="color7" Color="#EF6C96" />
    <SolidColorBrush x:Key="color8" Color="#78A3FC" />
    <SolidColorBrush x:Key="color9" Color="#07F3C0" />
    <SolidColorBrush x:Key="color10" Color="#FBA1AA" />
    <SolidColorBrush x:Key="color11" Color="#008CFF" />
    <!--  Font Colors  -->
    <SolidColorBrush x:Key="titleColor1" Color="#E0E1F1" />
    <SolidColorBrush x:Key="titleColor2" Color="#D5CFF5" />
    <SolidColorBrush x:Key="titleColor3" Color="#BCBEE0" />
    <SolidColorBrush x:Key="plainTextColor1" Color="#9497CD" />
    <SolidColorBrush x:Key="plainTextColor2" Color="#7C80C2" />
    <SolidColorBrush x:Key="plainTextColor3" Color="#7376BD" />
    <!--  Panel Colors  -->
    <SolidColorBrush x:Key="panelColor" Color="#1e4961" />
    <SolidColorBrush x:Key="panelOverColor" Color="#153547" />
    <SolidColorBrush x:Key="panelActiveColor" Color="#0f2b3b" />
    <!--  Button panel Colors  -->
    <SolidColorBrush x:Key="buttonClose" Color="#F03861" />
    <SolidColorBrush x:Key="buttonMinimize" Color="#0FF0FC   " />
    <SolidColorBrush x:Key="buttonMaximize" Color="#008CFF" />
    <!--  Border Rating Colors  -->
    <Color x:Key="colorThirdPlace">#FB539B</Color>
    <Color x:Key="colorFirstPlace">#FFC047</Color>
    <Color x:Key="colorSecondPlace">#4ADAEC</Color>
</ResourceDictionary>